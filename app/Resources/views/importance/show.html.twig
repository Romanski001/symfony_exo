{% extends 'base.html.twig' %}

{% block body %}
    <h1>Importance</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ importance.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ importance.nom }}</td>
            </tr>
            <tr>
                <th>Niveau</th>
                <td>{{ importance.niveau }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('importance_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('importance_edit', { 'id': importance.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
